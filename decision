import 'dart:math';


void main() {
quadraticEquation(11, -120, -11);
quadraticEquation(1, 6, 9);
quadraticEquation(1, 1, 1);
}
void quadraticEquation(a, b, c) 
{
  var x1;
  var x2;
  var x;
  var d=b*b-4*a*c;
  
  if (d>0)
  {x1=(-b+sqrt(d))/(2*a);
  x2=(-b-sqrt(d))/(2*a);
  print("x1=$x1" "x2=$x2");
  } 
   
   else if (d==0)
   {
     x=-b/(2*a);
     print("x=$x");
   }
  else if (d<0)
    
  {
    var n="нет корней";
    print(n);
  }
}
	
